{
  "name": "chat-client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-preset-react-app": "^2.2.0",
    "babel-register": "^6.22.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "flow-bin": "^0.46.0",
    "gh-pages": "^0.12.0",
    "husky": "^0.13.2",
    "jsdom": "^10.1.0",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.5.1",
    "react-scripts": "^0.9.5",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.2.0",
    "sw-precache": "^5.1.1"
  },
  "dependencies": {
    "emoji-shortnames": "^3.0.0",
    "lodash": "^4.16.6",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-emojione": "^3.1.7",
    "react-flip-move": "^2.9.10",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.0.0",
    "reconnecting-websocket": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --config=sw-precache-config.js",
    "test": "cross-env NODE_ENV=test ava",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "./node_modules/.bin/prettier --tab-width 4 --single-quote --trailing-comma es5 --bracket-spacing false --print-width 120 --write --parser flow",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
